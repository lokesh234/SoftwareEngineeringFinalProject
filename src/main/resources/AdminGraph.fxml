<?import net.kurobako.gesturefx.GesturePane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c20.teamU.Administration.GraphEditController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <AnchorPane prefHeight="1080" prefWidth="1920" BorderPane.alignment="CENTER">
                        <GesturePane AnchorPane.rightAnchor="0d" AnchorPane.bottomAnchor="0d" AnchorPane.topAnchor="0d" AnchorPane.leftAnchor="0d">
                           <AnchorPane fx:id="NodesPane">
                           <ImageView >
                              <image>
                                 <Image url="@png_files/Hospital.png" />
                              </image>
                           </ImageView>
                           </AnchorPane>
                        </GesturePane>
            </AnchorPane>
         </center>
         <top>
            <HBox prefHeight="100.0" style="-fx-background-color: #302D26;" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                     <children>
                        <Label text="View Graph" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="36.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="490.0" HBox.hgrow="ALWAYS">
                     <children>
                        <JFXButton fx:id="backButton" onAction="#backToAdmin" style="-fx-background-color: #474338;" text="Back" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI" size="18.0" />
                           </font>
                        </JFXButton>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </HBox>
         </top>
         <left>
            <VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #807765;" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="statusLabel1" text="SEARCH" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="18.0" />
                           </font>
                        </Label>
                        <JFXTextField focusColor="#474338" style="-fx-background-color: white;" unFocusColor="GREY" VBox.vgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </VBox>
                  <Line endX="100.0" startX="-100.0" />
                  <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="MODE SELECT" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="18.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <JFXButton fx:id="nodeModeButton" style="-fx-background-color: #474338;" text="NODE" textFill="WHITE" onAction="#enterNodeMode">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="edgeModeButton" style="-fx-background-color: #474338;" text="EDGE" textFill="WHITE" onAction="#exitNodeMode">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                           </children>
                        </HBox>
                        <VBox alignment="CENTER" spacing="10.0">
                           <children>
                              <JFXButton fx:id="addButton" style="-fx-background-color: #474338;" text="ADD" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="editButton" style="-fx-background-color: #474338;" text="EDIT" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="removeButton" style="-fx-background-color: red;" text="REMOVE" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <Line endX="100.0" startX="-100.0" />
                  <VBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="statusLabel" text="MODIFY" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="18.0" />
                           </font>
                        </Label>
                        <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <JFXButton fx:id="startButton" style="-fx-background-color: #474338;" text="Select Start" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <Label fx:id="startLabel" text="Node LongName" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <JFXButton fx:id="endButton" style="-fx-background-color: #474338;" text="Select End" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <Label fx:id="endLabel" text="Node LongName" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <JFXButton fx:id="posButton" style="-fx-background-color: #474338;" text="Select Posn" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </JFXButton>
                              <Label fx:id="posLabel" text="X, Y" textFill="WHITE">
                                 <font>
                                    <Font name="Segoe UI" size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </left>
         <right>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <JFXButton text="Î›">
                     <font>
                        <Font name="Segoe UI Bold" size="36.0" />
                     </font>
                  </JFXButton>
                  <Label text="#">
                     <font>
                        <Font name="Segoe UI Bold" size="36.0" />
                     </font>
                  </Label>
                  <JFXButton text="V">
                     <font>
                        <Font name="Segoe UI Bold" size="36.0" />
                     </font>
                  </JFXButton>
               </children>
            </VBox>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
