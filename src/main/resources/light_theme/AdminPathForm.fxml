<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="windowScreen" stylesheets="@light.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c20.teamU.Administration.ChoosePathController">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER" styleClass="titleBar" stylesheets="@light.css" VBox.vgrow="ALWAYS">
                    <children>
                  <Label prefHeight="95.0" prefWidth="383.0" text="Choose Pathfinding Algorithm">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                  </Label>
                    </children>
                    <padding>
                        <Insets right="18.0" top="7.0" />
                    </padding>
                </HBox>
                <VBox alignment="CENTER" prefHeight="267.0" prefWidth="428.0" spacing="10.0" stylesheets="@light.css" VBox.vgrow="ALWAYS">
               <children>
                  <JFXRadioButton fx:id="sta" text="A*" VBox.vgrow="ALWAYS">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets right="79.0" />
                     </VBox.margin>
                  </JFXRadioButton>
                  <JFXRadioButton fx:id="bfs" text="Breadth-first">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets right="-8.0" />
                     </VBox.margin>
                  </JFXRadioButton>
                  <JFXRadioButton fx:id="dfs" text="Depth-first">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets right="11.0" />
                     </VBox.margin>
                  </JFXRadioButton>
                  <JFXRadioButton fx:id="djikstra" text="Dijkstra's">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets right="22.0" />
                     </VBox.margin>
                  </JFXRadioButton>
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
                </VBox>
            <HBox alignment="CENTER" spacing="30.0" stylesheets="@light.css" VBox.vgrow="ALWAYS">
               <children>
                  <JFXButton fx:id="done" onAction="#set" stylesheets="@light.css" text="Done" textFill="WHITE">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="done1" onAction="#set" stylesheets="@light.css" text="Cancel" textFill="WHITE">
                     <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </HBox>
            </children>
        </VBox>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="153.0" spacing="20.0">
         <children>
            <HBox alignment="CENTER" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
